QVector<QVector<int» convertToAdjacencyMatrix(QList<QList<QString» adjacencyList, QList<QString> nodes)
{
    int n = nodes.size();
    QVector<QVector<int» adjacencyMatrix(n, QVector<int>(n, 0));

    for (int i = 0; i < adjacencyList.size(); i++) {
        QString node1 = adjacencyList[i][0];
        int index1 = nodes.indexOf(node1);

        for (int j = 1; j < adjacencyList[i].size(); j++) {
            QString node2 = adjacencyList[i][j];
            int index2 = nodes.indexOf(node2);

            adjacencyMatrix[index1][index2] = 1;
        }
    }

    return adjacencyMatrix;
}
