#include <iostream>
#include <QString>
#include <cmath>

int main() {
    QString expression = "1+cos(2)*3 + 1.0e-10 - 23/( sqrt(9)-2)";
    QString result;

    QStringList tokens = expression.split(' ', QString::SkipEmptyParts);

    for (const QString& token : tokens) {
        if (token.contains('e') || token.contains('E')) {
            double number = token.toDouble();
            result += QString::number(number, 'g', 15);
        }
        else {
            result += token;
        }
        result += " ";
    }

    std::cout << "Original expression: " << expression.toStdString() << std::endl;
    std::cout << "Transformed expression: " << result.trimmed().toStdString() << std::endl;

    return 0;
}
