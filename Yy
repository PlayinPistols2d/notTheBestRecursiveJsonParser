#include <QString>
#include <QStack>

double calc(const QString& expr)
{
    QStack<double> stack;
    QStringList tokens = expr.split(' ');
    foreach (const QString& token, tokens) {
        if (token == "+") {
            double op2 = stack.pop();
            double op1 = stack.pop();
            stack.push(op1 + op2);
        } else if (token == "-") {
            double op2 = stack.pop();
            double op1 = stack.pop();
            stack.push(op1 - op2);
        } else if (token == "*") {
            double op2 = stack.pop();
            double op1 = stack.pop();
            stack.push(op1 * op2);
        } else if (token == "/") {
            double op2 = stack.pop();
            double op1 = stack.pop();
            stack.push(op1 / op2);
        } else {
            double number = token.toDouble();
            stack.push(number);
        }
    }
    return stack.pop();
}

int main()
{
    double result = calc("53 26 2 - * 2 /");
    qDebug() << "Result: " << result;
    return 0;
}
